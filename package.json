    "pad": "^2.3.0",
    "pkg": "^4.4.2",
    "prompt": "^1.0.0",
    "request": "^2.88.0",
    "request-progress": "^3.0.0",
    "sanitize-filename": "^1.6.1",
    "xmlbuilder": "^12.0.0",
